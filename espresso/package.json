    "colors": "^1.4.0",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "ts-node": "3.3.0",
    "typescript": "3.8"
  },
  "dependencies": {
    "apollo-server-core": "^3.1.2",
    "apollo-server-express": "3.1.2",
